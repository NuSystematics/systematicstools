@PACKAGE_INIT@

set(systematicstools_VERSION @PROJECT_VERSION@)

find_package(ROOT 6.10 REQUIRED)

include(${CMAKE_CURRENT_LIST_DIR}/systtools-targets.cmake)

set(systematicstools_FOUND FALSE)

if(NOT TARGET systematicstools::all)
  cmessage(WARNING "Expected to find target systematicstools::all in ${CMAKE_CURRENT_LIST_DIR}/systtools-targets.cmake")
  set(systematicstools_FOUND FALSE)
  return()
endif()